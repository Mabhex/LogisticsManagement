@{
    ViewData["Title"] = "";
}
<h1>@ViewData["Title"]</h1>

<div class="row">
    <div class="col-lg-12">
        <p>
            <i class="fa fa-paper-plane-o"></i> Orders
            <hr />
        </p>
    </div>
</div>

<div class="row">
    <div class="col-sm-2">
        <a asp-controller="Order" asp-action="Index">
            <div class="dashboard-task-button">
                <img src="~/images/flaticons-64x64/logistics-delivery.png" />
                <div>Delivery</div>
            </div>
        </a>
    </div>
    <div class="col-sm-2">
        <a asp-controller="Home" asp-action="Privacy">
            <div class="dashboard-task-button">
                <img src="~/images/flaticons-64x64/effort.png" />
                <div>Misc.</div>
            </div>
        </a>
    </div>
    <div class="col-sm-2">
        <a asp-controller="Home" asp-action="Privacy">
            <div class="dashboard-task-button">
                <img src="~/images/flaticons-64x64/boxes.png" />
                <div>Storage</div>
            </div>
        </a>
    </div>
    <div class="col-sm-2">
        <a asp-controller="Home" asp-action="Privacy">
            <div class="dashboard-task-button">
                <img src="~/images/flaticons-64x64/delivery-cart.png" />
                <div>Deliver</div>
            </div>
        </a>
    </div>
</div>

<br /><br />


<div class="row">
    <div class="col-lg-12">
        <p>
            <i class="fa fa-building-o"></i> Invoice & Payments
            <hr />
        </p>
    </div>
</div>

<div class="row">
    <div class="col-sm-2">
        <a asp-controller="Invoice" asp-action="Index">
            <div class="dashboard-task-button">
                <img src="~/images/flaticons-64x64/invoice.png" />
                <div>Invoice</div>
            </div>
        </a>
    </div>
    <div class="col-sm-2">
        <a asp-controller="Invoice" asp-action="InvoicePayment">
            <div class="dashboard-task-button">
                <img src="~/images/flaticons-64x64/online-payment.png" />
                <div>Payments</div>
            </div>
        </a>
    </div>
</div>







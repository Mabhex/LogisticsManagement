@model LogisticsManagement_Poco.Lms_ConfigurationPoco


@{
    ViewData["Title"] = "Configuration";
}

<div class="row">
    <div class="col-sm-12">
        <div class="card">
            <div class="card-header" style="background-color: #3b80a9;color: #fff;">
                <strong><i class="fa fa-sliders"></i> Configuration</strong>
            </div>

            <div class="card-body">
                <div class="row">
                    <div class="form-group col-sm-1">
                        <input class="form-control" id="txtConfigId" placeholder="Id" value="@Model.Id" disabled />
                    </div>

                    <div class="form-group col-sm-1" style="padding-left: 0px;">
                        <button id="btnFind" class="btn btn-secondary"><i class="fa fa-search"></i></button>
                    </div>
                </div>

                <div class="row">
                    <div class="form-group col-sm-2">
                        <select class="form-control">
                            @foreach (var item in ViewBag.TaxToCall)
                            {
                                var selected = false;
                                if (item.Id == 1)
                                {
                                    selected = true;
                                }
                                <option value="@item.Id" selected="@selected">@item</option>
                            }
                        </select>
                    </div>
                    <div class="form-group col-sm-2">
                       
                    </div>
                </div>



                <hr />
                <button type="button" class="btn btn-success"><i class="fa fa-save"></i> Save</button>

            </div>
        </div>
    </div>
</div>

